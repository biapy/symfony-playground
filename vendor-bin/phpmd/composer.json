{
  "require-dev": {
    "phpmd/phpmd": "@stable"
  },
  "scripts": {
    "post-install-cmd": [
      "@install-link"
    ],
    "install-link": [
      "mkdir --parent '../../vendor/bin' && test -e '../../vendor/bin/phpmd' || ln --symbolic '../../vendor-bin/phpmd/vendor/bin/phpmd' '../../vendor/bin/phpmd'"
    ]
  },
  "scripts-descriptions": {
    "install-link": "Install composer bin link"
  }
}